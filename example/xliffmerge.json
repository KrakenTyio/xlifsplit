{
  "xliffmergeOptions": {
    "srcDir": "locale",
    "genDir": "locale",
    "i18nFile": "messages.xlf",
    "i18nFormat": "xlf2",
    "encoding": "UTF-8",
    "beautifyOutput": true,
    "defaultLanguage": "en-GB",
    "useSourceAsTarget": true,
    "targetPraefix": "%%",
    "languages": ["en-GB", "nl-BE", "de-DE"]
  }
}
